{
  "name": "rte-group",
  "version": "1.0.0",
  "description": "Транспортная компания, специализирующаяся на крупногабаритных и негабаритных перевозках",
  "main": "index.js",
  "scripts": {
    "postinstall": "sh ./scripts/postinstall.sh",
    "build": "webpack --mode production ",
    "dev": "webpack server",
    "lint-fix": "npx eslint --fix \"**/*.{js,jsx}\"",
    "stylelint-fix": "npx prettier --write \"**/*.{css,scss}\" && npx stylelint --fix --custom-syntax postcss-scss \"**/*.{css,scss}\"",
    "prettier-fix": "npx prettier --write \"**/*.{json,md}\"",
    "htmlhint-fix": "npx prettier --write \"**/*.html\" && npx htmlhint \"**/*.html\"",
    "all-fix": "npm run lint-fix && npm run stylelint-fix && npm run prettier-fix && npm run htmlhint-fix",
    "lint-staged-fix": "npx lint-staged"
  },
  "keywords": [
    "HTML5",
    "CSS",
    "РТ Экспресс"
  ],
  "authors": [
