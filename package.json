    "*.{js,jsx}": "eslint --fix",
    "*.{css,scss}": [
      "prettier --write",
      "stylelint --fix --custom-syntax postcss-scss"
    ],
    "*.{json,md}": "prettier --write",
    "*.html": [
      "prettier --write",
      "htmlhint"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.8.1",
    "cssnano": "^6.0.1",
    "eslint": "^8.49.0",
    "html-webpack-plugin": "^5.5.3",
    "htmlhint": "^1.1.4",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "mini-css-extract-plugin": "^2.7.6",
    "node-sass": "^9.0.0",
    "normalize.css": "^8.0.1",
